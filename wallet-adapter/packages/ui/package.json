    "build": "pnpm build:types && tsup ./src/index.ts --format esm,cjs --sourcemap",
    "build:types": "tsc --build",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@mysten/sui.js": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.2.2",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.4",
    "@mysten/wallet-adapter-react": "workspace:*"
  }
}
