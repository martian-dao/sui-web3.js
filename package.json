{
  "name": "@martianwallet/sui-web3.js",
  "description": "Web3 SDK For Sui",
  "version": "0.2.2",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf tsconfig.tsbuildinfo ./dist",
    "publish_module": "npm run build && npm publish --public",
    "codegen:version": "node genversion.mjs",
    "dev": "pnpm build:tsup --watch",
    "build:custom": "node ./type_guards.mjs && pnpm build:types && tsup ./src/index.ts --format esm,cjs --sourcemap",
    "build": "node genversion.mjs && pnpm build:types && pnpm build:tsup",
    "build:tsup": "tsup ./src/index.ts --sourcemap",
    "build:types": "tsc --build",
    "doc": "typedoc",
    "vitest": "vitest",
    "test": "jest -- src/wallet-client.test.ts",
    "test:typecheck": "tsc -p ./test",
    "test:unit": "vitest run unit __tests__",
    "test:e2e": "wait-on http://127.0.0.1:9123 -l --timeout 120000 && vitest run e2e",
    "test:e2e:nowait": "vitest run e2e",
    "prepare:e2e": "cargo build --bin sui-test-validator --bin sui --profile dev && cross-env RUST_LOG=info,sui=error,anemo_tower=warn,consensus=off cargo run --bin sui-test-validator",
    "lint": "eslint --max-warnings=0 src test",
    "prettier:fix": "prettier --write .",
    "prepublishOnly": "pnpm build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "bugs": {
    "url": "https://github.com/MystenLabs/sui/issues/new"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "100 KB"
    }
  ],
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.11",
    "@types/tmp": "^0.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "jest": "^29.5.0",
    "mockttp": "^2.7.0",
    "prettier": "^2.8.7",
    "size-limit": "^8.2.4",
    "tmp": "^0.2.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-retry-promise": "^0.7.0",
    "tsup": "^6.7.0",
    "typedoc": "^0.24.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@mysten/bcs": "0.7.3",
    "@mysten/kiosk": "^0.3.3",
    "@noble/curves": "^1.0.0",
    "@noble/hashes": "^1.3.0",
    "@noble/secp256k1": "^1.7.1",
    "@open-rpc/client-js": "^1.8.1",
    "@scure/bip32": "^1.3.0",
    "@scure/bip39": "^1.2.0",
    "@suchipi/femver": "^1.0.0",
    "bignumber.js": "^9.1.1",
    "isomorphic-fetch": "^3.0.0",
    "jayson": "^4.0.0",
    "msw": "^1.2.1",
    "rpc-websockets": "^7.5.1",
    "superstruct": "^1.0.3",
    "tweetnacl": "^1.0.3"
  }
}
